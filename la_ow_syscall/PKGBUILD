pkgname=la_ow_syscall-dkms
pkgver=0.1.0
pkgrel=1
pkgdesc="loongarch old-world syscall module sources"
arch=(any)
url="https://github.com/AOSC-Dev/la_ow_syscall"
license=(GPL2)
depends=(dkms)
source=("git+https://github.com/AOSC-Dev/la_ow_syscall.git"
    dkms.conf
    Makefile
    )
sha256sums=('SKIP'
            '23a107671bea57f50956fca647999976034293bb49385c5dc881167c96c622c5'
            'a7b7e38907776ec3586aee84d18c3cadb8350f0b13722ca79f461390fea81d92')

package() {
  cd la_ow_syscall
  install -Dm644 fsstat.c  fsstat.h  Kbuild  la_ow_syscall_main.c  signal.c  signal.h -t "${pkgdir}"/usr/src/${pkgbase}-${pkgver}/
  install -Dm644 ${srcdir}/dkms.conf ${srcdir}/Makefile -t "${pkgdir}"/usr/src/${pkgbase}-${pkgver}/
}
